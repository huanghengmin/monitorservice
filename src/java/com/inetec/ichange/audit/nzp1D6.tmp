public class AuditDestDb {

    private String m_dbName = null;
    private String m_tableName = null;
    private int m_number = 0;

    public void setDbName(String m_dbName) {
        this.m_dbName = m_dbName;
    }

    public void setTableName(String m_tableName) {
        this.m_tableName = m_tableName;
    }

    public void setNumber(int m_number) {
        this.m_number = m_number;
    }

    protected AuditDestDb(String dbname, String tableName, int number) {
        m_dbName = dbname;
        m_tableName = tableName;
        m_number = number;
    }

    public String getDbName() {
        return m_dbName;
    }

    public String getTableName() {
        return m_tableName;
    }

    public int getNumber() {
        return m_number;
    }
}
